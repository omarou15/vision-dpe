{
  "name": "vision-dpe",
  "version": "0.1.0",
  "description": "Vision DPE - Logiciel de Diagnostic de Performance Énergétique conforme 3CL",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint src/ --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "DPE",
    "3CL",
    "diagnostic",
    "énergie",
    "ADEME"
  ],
  "author": "EnergyCo",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "fast-xml-parser": "^4.3.4",
    "zod": "^3.22.4"
  },
  "jest-junit": {
    "outputDirectory": "./reports",
    "outputName": "junit.xml"
  }
}
